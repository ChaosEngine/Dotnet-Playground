    <TargetFramework>netcoreapp2.0</TargetFramework>
    <IsPackable>false</IsPackable>
    <AssemblyName>AspNetCore.ExistingDb.Tests</AssemblyName>
    <RootNamespace>AspNetCore.ExistingDb.Tests</RootNamespace>
