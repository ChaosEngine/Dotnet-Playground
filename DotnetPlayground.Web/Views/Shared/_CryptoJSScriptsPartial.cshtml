<environment include="Development">
	<script type="text/javascript">
		var libs2Load = ['../../lib/forge/forge.min.js', 'shared.js'];
	</script>
	<script src="~/lib/forge/forge.min.js"></script>
</environment>
<environment exclude="Development">
	<script type="text/javascript">
		var libs2Load = [];
	</script>
	<script src="https://cdn.jsdelivr.net/npm/node-forge@1.1.0/dist/forge.min.js" integrity="sha256-30g8Vx77OikFjevJtYdrrE0tMVsQymL5VfUHqILzwWg=" crossorigin="anonymous"
			asp-fallback-src="~/lib/forge/forge.min.js"
			asp-fallback-test="(window.forge && window.forge.sha256 && libs2Load.push('https://cdn.jsdelivr.net/npm/node-forge@1.1.0/dist/forge.min.js'))"></script>
	<script type="text/javascript">
		if(libs2Load.length <= 0)
		{
			libs2Load = ['../../lib/forge/forge.min.js', 'shared.min.js'];
		}
		else
			libs2Load.push('shared.min.js');
	</script>
</environment>
