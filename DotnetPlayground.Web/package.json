{
	"private": true,
	"eslintConfig": {
		"env": {
			"es6": true,
			"browser": true,
			"jquery": true,
			"worker": true
		},
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaVersion": 2020,
			"sourceType": "module"
		},
		"rules": {
			"indent": [
				"off",
				"tab"
			],
			"linebreak-style": [
				"off",
				"unix"
			],
			"quotes": [
				"off",
				"double"
			],
			"semi": [
				"error",
				"always"
			],
			"eqeqeq": 2,
			"comma-dangle": 1,
			"no-console": 1,
			"no-debugger": 1,
			"no-extra-semi": 1,
			"no-extra-parens": 0,
			"no-irregular-whitespace": 1,
			"no-undef": 2,
			"no-unused-vars": 1,
			"semi-spacing": 1,
			"valid-jsdoc": [
				2,
				{
					"requireReturn": false
				}
			]
		}
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"eslint": "8.25.0",
		"fs-extra": "10.1.0",
		"gulp": "4.0.2",
		"gulp-terser": "2.1.0",
		"gulp-concat": "2.6.1",
		"gulp-clean-css": "4.3.0",
		"gulp-rename": "2.0.0",
		"gulp-sass": "5.1.0",
		"gulp-sourcemaps": "3.0.0",
		"sass": "1.55.0",
		"gulp-header": "2.0.9",
		"rimraf": "3.0.2",
		"concaveman": "1.2.1",
		"webpack": "5.74.0",
		"webpack-stream": "7.0.0",
		"poly-decomp": "0.3.0",
		"worker-plugin": "5.0.1",
		"@playwright/test": "1.27.1"
	},
	"dependencies": {
		"bootstrap": "5.2.2",
		"bootstrap-table": "1.21.1",
		"node-forge": "1.3.1",
		"jquery": "3.6.1",
		"jquery-validation": "1.19.5",
		"jquery-validation-unobtrusive": "4.0.0",
		"blueimp-gallery": "3.4.0",
		"video.js": "7.20.3",
		"qrcodejs": "1.0.0",
		"@microsoft/signalr": "6.0.10",
		"@microsoft/signalr-protocol-msgpack": "6.0.10",
		"msgpack5": "6.0.2",
		"ace-builds": "1.12.3",
		"chance": "1.1.9"
	},
	"overrides": {
		"clean-css": ">=5.3.1"
	},
	"scripts": {
		"postinstall": "npx gulp postinstall",
		"gulp": "npx gulp"
	}
}
