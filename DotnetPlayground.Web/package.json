{
	"private": true,
	"eslintConfig": {
		"env": {
			"es6": true,
			"browser": true,
			"jquery": true,
			"worker": true
		},
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaVersion": 2022,
			"sourceType": "module"
		},
		"rules": {
			"indent": [
				"off",
				"tab"
			],
			"linebreak-style": [
				"off",
				"unix"
			],
			"quotes": [
				"off",
				"double"
			],
			"semi": [
				"error",
				"always"
			],
			"eqeqeq": 2,
			"comma-dangle": 1,
			"no-console": 1,
			"no-debugger": 1,
			"no-extra-semi": 1,
			"no-extra-parens": 0,
			"no-irregular-whitespace": 1,
			"no-undef": 2,
			"no-unused-vars": 1,
			"semi-spacing": 1,
			"valid-jsdoc": [
				2,
				{
					"requireReturn": false
				}
			]
		}
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@aptuitiv/gulp-clean-css": "4.4.3",
		"@playwright/test": "1.43.1",
		"concaveman": "1.2.1",
		"density-clustering": "1.3.0",
		"eslint": "9.1.1",
		"fs-extra": "11.2.0",
		"gulp": "5.0.0",
		"gulp-concat": "2.6.1",
		"gulp-header": "2.0.9",
		"gulp-rename": "2.0.0",
		"gulp-sass": "5.1.0",
		"gulp-sourcemaps": "3.0.0",
		"gulp-terser": "2.1.0",
		"javascript-astar": "0.4.1",
		"sass": "1.75.0",
		"terser-webpack-plugin": "5.3.10",
		"tiny-glob": "0.2.9",
		"webpack": "5.91.0",
		"webpack-stream": "7.0.0",
		"worker-plugin": "5.0.1"
	},
	"dependencies": {
		"@microsoft/signalr": "8.0.0",
		"@microsoft/signalr-protocol-msgpack": "8.0.0",
		"ace-builds": "1.33.1",
		"blueimp-gallery": "3.4.0",
		"bootstrap": "5.3.3",
		"bootstrap-table": "1.22.4",
		"chance": "1.1.11",
		"jquery": "3.7.1",
		"jquery-validation": "1.20.0",
		"jquery-validation-unobtrusive": "4.0.0",
		"msgpack5": "6.0.2",
		"node-forge": "1.3.1",
		"qrcodejs": "1.0.0",
		"video.js": "8.10.0"
	},
	"scripts": {
		"postinstall": "npx gulp postinstall",
		"gulp": "npx gulp"
	}
}
