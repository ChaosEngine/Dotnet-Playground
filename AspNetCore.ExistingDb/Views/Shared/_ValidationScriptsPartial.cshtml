@inject  Microsoft.Extensions.Configuration.IConfiguration Conf
<environment names="Development">@*
									 <script src="@Conf.AppRootPath()lib/jquery-validation/dist/jquery.validate.js"></script>
									 <script src="@Conf.AppRootPath()lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

	*@<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js" integrity="sha256-UOSXsAgYN43P/oVrmU+JlHtiDGYWN2iHnJuKY9WD+Jg=" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js" integrity="sha256-EyQxcbH1l25095ZH9hKh2Hm/pgaBaiBPcqgzwOifJpo=" crossorigin="anonymous"></script>
</environment>
<environment names="Staging,Production">
	<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js" integrity="sha256-UOSXsAgYN43P/oVrmU+JlHtiDGYWN2iHnJuKY9WD+Jg=" crossorigin="anonymous"
			asp-fallback-src="@Conf.AppRootPath()lib/jquery-validation/dist/jquery.validate.min.js"
			asp-fallback-test="window.jQuery && window.jQuery.validator">
	</script>
	<script src="https://cdn.jsdelivr.net/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js" integrity="sha256-EyQxcbH1l25095ZH9hKh2Hm/pgaBaiBPcqgzwOifJpo=" crossorigin="anonymous"
			asp-fallback-src="@Conf.AppRootPath()lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"
			asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
	</script>
</environment>
