@inject  Microsoft.Extensions.Configuration.IConfiguration Conf
<environment names="Development">
	<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
	@*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.min.js" integrity="sha256-F6h55Qw6sweK+t7SiOJX+2bpSAa3b/fnlrVCJvmEj1A=" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js" integrity="sha256-EyQxcbH1l25095ZH9hKh2Hm/pgaBaiBPcqgzwOifJpo=" crossorigin="anonymous"></script>*@
</environment>
<environment names="Staging,Production">
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.min.js" integrity="sha256-F6h55Qw6sweK+t7SiOJX+2bpSAa3b/fnlrVCJvmEj1A=" crossorigin="anonymous"
			asp-fallback-src="~/lib/jquery-validation/dist/jquery.validate.min.js"
			asp-fallback-test="window.jQuery && window.jQuery.validator">
	</script>
	<script src="https://cdn.jsdelivr.net/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js" integrity="sha256-EyQxcbH1l25095ZH9hKh2Hm/pgaBaiBPcqgzwOifJpo=" crossorigin="anonymous"
			asp-fallback-src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"
			asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
	</script>
</environment>
