<environment include="Development">
	<script type="text/javascript">
		var libs2Load = ['../../lib/forge/dist/forge.min.js', 'shared.js'];
	</script>
	<script src="~/lib/forge/dist/forge.min.js"></script>
</environment>
<environment exclude="Development">
	<script type="text/javascript">
		var libs2Load = [];
	</script>
	<script src="https://cdn.jsdelivr.net/npm/node-forge@0.9.2/dist/forge.min.js" integrity="sha256-yXrGIwDKUYCS7/LLjJjlicO5+zzXbW9CAeHpOLEi/rk=" crossorigin="anonymous"
			asp-fallback-src="~/lib/forge/dist/forge.min.js"
			asp-fallback-test="(window.forge && window.forge.sha256 && libs2Load.push('https://cdn.jsdelivr.net/npm/node-forge@0.9.2/dist/forge.min.js'))"></script>
	<script type="text/javascript">
		if(libs2Load.length <= 0)
		{
			libs2Load = ['../../lib/forge/dist/forge.min.js', 'shared.js'];
		}
		else
			libs2Load.push('shared.js');
	</script>
</environment>
