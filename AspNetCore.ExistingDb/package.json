{
	"name": "aspnetcore.existingdb",
	"version": "1.0.3-custom",
	"private": true,
	"eslintConfig": {
		"env": {
			"es6": true,
			"browser": true,
			"jquery": true,
			"worker": true
		},
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaVersion": 2017
		},
		"rules": {
			"indent": [
				"off",
				"tab"
			],
			"linebreak-style": [
				"off",
				"unix"
			],
			"quotes": [
				"off",
				"double"
			],
			"semi": [
				"error",
				"always"
			],
			"eqeqeq": 2,
			"comma-dangle": 1,
			"no-console": 1,
			"no-debugger": 1,
			"no-extra-semi": 1,
			"no-extra-parens": 0,
			"no-irregular-whitespace": 1,
			"no-undef": 2,
			"no-unused-vars": 1,
			"semi-spacing": 1,
			"valid-jsdoc": [
				2,
				{
					"requireReturn": false
				}
			]
		}
	},
	"devDependencies": {
		"@babel/core": "7.7.7",
		"@babel/plugin-transform-runtime": "7.7.6",
		"@babel/preset-env": "7.7.7",
		"eslint": "6.7.2",
		"fs-extra": "8.1.0",
		"gulp": "4.0.2",
		"gulp-babel": "8.0.0",
		"gulp-terser": "1.2.0",
		"gulp-concat": "2.6.1",
		"gulp-clean-css": "4.2.0",
		"gulp-rename": "2.0.0",
		"rimraf": "3.0.0"
	},
	"dependencies": {
		"bootstrap": "4.4.1",
		"bootstrap-table": "1.15.5",
		"node-forge": "0.9.1",
		"jquery": "3.4.1",
		"jquery-validation": "1.19.1",
		"jquery-validation-unobtrusive": "3.2.11",
		"blueimp-gallery": "2.36.0",
		"video.js": "7.6.6",
		"qrcodejs": "1.0.0",
		"@microsoft/signalr": "3.1.0",
		"@microsoft/signalr-protocol-msgpack": "3.1.0",
		"msgpack5": "4.2.1"
	},
	"scripts": {
		"postinstall": "node -e \"require('fs-extra').copySync('node_modules/bootstrap', 'wwwroot/lib/bootstrap'); require('fs-extra').copySync('node_modules/bootstrap-table', 'wwwroot/lib/bootstrap-table'); require('fs-extra').copySync('node_modules/node-forge', 'wwwroot/lib/forge'); require('fs-extra').copySync('node_modules/jquery', 'wwwroot/lib/jquery'); require('fs-extra').copySync('node_modules/jquery-validation', 'wwwroot/lib/jquery-validation'); require('fs-extra').copySync('node_modules/jquery-validation-unobtrusive', 'wwwroot/lib/jquery-validation-unobtrusive'); require('fs-extra').copySync('node_modules/blueimp-gallery', 'wwwroot/lib/blueimp-gallery'); require('fs-extra').copySync('node_modules/video.js', 'wwwroot/lib/video.js'); require('fs-extra').copySync('node_modules/qrcodejs', 'wwwroot/lib/qrcodejs'); require('fs-extra').copySync('node_modules/@microsoft/signalr', 'wwwroot/lib/signalr'); require('fs-extra').copySync('node_modules/@microsoft/signalr-protocol-msgpack', 'wwwroot/lib/signalr-protocol-msgpack'); require('fs-extra').copySync('node_modules/msgpack5', 'wwwroot/lib/msgpack5');\""
	}
}
